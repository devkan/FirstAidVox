# FirstAidVox Backend Environment Configuration Template
# Copy this file to .env and fill in the required values

# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_APPLICATION_CREDENTIALS=path/to/service-account-key.json

# Google Maps API Configuration
GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Vertex AI Configuration
VERTEX_AI_LOCATION=us-central1
GEMINI_MODEL_NAME=gemini-1.5-flash-001

# Application Configuration
LOG_LEVEL=INFO
ENVIRONMENT=development
DEBUG=false

# API Configuration
CORS_ORIGINS=http://localhost:3000,http://localhost:8080
MAX_IMAGE_SIZE_MB=10
REQUEST_TIMEOUT_SECONDS=30

# Performance Configuration
MAX_CONCURRENT_REQUESTS=100
RESPONSE_TIME_LIMIT_SECONDS=3

# Production Environment Variables (for Cloud Run)
# These should be set in the deployment configuration, not in .env files

# ENVIRONMENT=production
# LOG_LEVEL=INFO
# GOOGLE_CLOUD_PROJECT=your-production-project-id
# VERTEX_AI_LOCATION=us-central1
# GOOGLE_MAPS_API_KEY=your-production-api-key
# CORS_ORIGINS=https://yourdomain.com,https://app.yourdomain.com
# MAX_IMAGE_SIZE_MB=10
# REQUEST_TIMEOUT_SECONDS=30
# MAX_CONCURRENT_REQUESTS=100
# RESPONSE_TIME_LIMIT_SECONDS=3